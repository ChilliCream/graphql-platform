<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <IsPackable>false</IsPackable>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HotChocolate.AspNetCore" Version="11.0.0-dev.14" />
    <PackageReference Include="HotChocolate.AspNetCore.Authorization" Version="11.0.0-dev.14" />
    <PackageReference Include="HotChocolate.Data" Version="11.0.0-dev.14" />
  </ItemGroup>

</Project>
