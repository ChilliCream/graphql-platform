schema {
  query: Query
}

type Query {
  foo: String
}

input FooSimpleFilter {
  AND: [FooSimpleFilter!]
  OR: [FooSimpleFilter!]
  barInt32_some: ISingleFilterOfInt32Filter
}

input ISingleFilterOfInt32Filter {
  AND: [ISingleFilterOfInt32Filter!]
  OR: [ISingleFilterOfInt32Filter!]
  element: Int
}
