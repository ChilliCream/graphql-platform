{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/PublicAPI*.txt": true
  },
  "editor.rulers": [120],
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.*proj": "xml",
    "*.props": "xml",
    "*.targets": "xml",
    "*.tasks": "xml"
  },
  "dotnet.defaultSolution": "src/All.sln"
}
