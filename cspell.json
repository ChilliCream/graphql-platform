{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["csharp", "custom"],
  "dictionaryDefinitions": [
    {
      "name": "custom",
      "path": "./dictionary.txt"
    }
  ],
  "ignorePaths": [
    "__snapshots__/",
    "*.Designer.cs",
    "*.DotSettings*",
    "*.drawio",
    "**/bin/",
    "**/obj/",
    "packages.lock.json"
  ],
  "ignoreRegExpList": [
    "featuredVideoId:(.*)", // video hash
    "videoId=\"(.*)\"" // video hash
  ]
}
