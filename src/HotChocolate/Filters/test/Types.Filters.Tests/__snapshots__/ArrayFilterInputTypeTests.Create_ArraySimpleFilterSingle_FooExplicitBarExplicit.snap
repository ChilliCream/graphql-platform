schema {
  query: Query
}

type Query {
  foo: String
}

input FooSimpleFilter {
  AND: [FooSimpleFilter!]
  OR: [FooSimpleFilter!]
  barSingle_some: ISingleFilterOfSingleFilter
}

input ISingleFilterOfSingleFilter {
  AND: [ISingleFilterOfSingleFilter!]
  OR: [ISingleFilterOfSingleFilter!]
  element: Float
}
