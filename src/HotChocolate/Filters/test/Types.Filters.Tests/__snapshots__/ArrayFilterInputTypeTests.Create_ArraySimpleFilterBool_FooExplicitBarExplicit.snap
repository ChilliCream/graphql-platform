schema {
  query: Query
}

type Query {
  foo: String
}

input FooSimpleFilter {
  AND: [FooSimpleFilter!]
  OR: [FooSimpleFilter!]
  barBool_some: ISingleFilterOfBooleanFilter
}

input ISingleFilterOfBooleanFilter {
  AND: [ISingleFilterOfBooleanFilter!]
  OR: [ISingleFilterOfBooleanFilter!]
  element: Boolean
}
