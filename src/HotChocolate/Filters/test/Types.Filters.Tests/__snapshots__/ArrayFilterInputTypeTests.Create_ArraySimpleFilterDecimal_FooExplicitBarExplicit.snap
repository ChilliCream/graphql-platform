schema {
  query: Query
}

type Query {
  foo: String
}

input FooSimpleFilter {
  AND: [FooSimpleFilter!]
  OR: [FooSimpleFilter!]
  barDecimal_some: ISingleFilterOfDecimalFilter
}

input ISingleFilterOfDecimalFilter {
  AND: [ISingleFilterOfDecimalFilter!]
  OR: [ISingleFilterOfDecimalFilter!]
  element: Decimal
}

"The built-in `Decimal` scalar type."
scalar Decimal
