schema {
  query: Query
  mutation: Mutation
}

type Query {
  findPetById(id: Long): Pet
  findpets(limit: Int tags: [String]): [Pet]
}

type Mutation {
  addpet(input: addPetInputType): addPetPayload
  deletepet(input: deletePetInputType): deletePetPayload
}

type addPetPayload {
  id: Long
  name: String
  tag: String
}

type deletePetPayload {
  success: Boolean
}

type Pet {
  id: Long
  name: String
  tag: String
}

input addPetInputType {
  NewPet: NewPetInputType
}

input deletePetInputType {
  id: Long
}

input NewPetInputType {
  name: String
  tag: String
}
