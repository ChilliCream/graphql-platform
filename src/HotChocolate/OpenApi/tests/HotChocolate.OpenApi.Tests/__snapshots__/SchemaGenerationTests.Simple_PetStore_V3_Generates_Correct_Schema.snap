schema {
  query: Query
  mutation: Mutation
}

type Mutation {
  addpet(input: addPetInputType): addPetPayload
  deletepet(input: deletePetInputType): deletePetPayload
}

type Pet {
  id: Long
  name: String
  tag: String
}

type Query {
  findPetById(id: Long): Pet
  findpets(limit: Int tags: String): Pet
}

type addPetPayload {
  id: Long
  name: String
  tag: String
}

type deletePetPayload {
  success: Boolean
}

input NewPetInputType {
  name: String
  tag: String
}

input addPetInputType {
  NewPet: NewPetInputType
}

input deletePetInputType {
  id: Long
}

"The `Long` scalar type represents non-fractional signed whole 64-bit numeric values. Long can represent values between -(2^63) and 2^63 - 1."
scalar Long
