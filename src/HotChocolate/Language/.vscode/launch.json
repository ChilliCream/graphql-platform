  "version": "0.2.0",
  "configurations": [
      {
          "name": ".NET Core Launch (console)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/benchmark/Language.Visitors.Benchmarks/bin/Debug/netcoreapp3.1/HotChocolate.Language.Visitors.Benchmarks.dll",
          "args": [],
          "cwd": "${workspaceFolder}/benchmark/Language.Visitors.Benchmarks",
          "console": "internalConsole",
          "stopAtEntry": false
      },
      {
          "name": ".NET Core Attach",
          "type": "coreclr",
          "request": "attach",
          "processId": "${command:pickProcess}"
      }
  ]
}
