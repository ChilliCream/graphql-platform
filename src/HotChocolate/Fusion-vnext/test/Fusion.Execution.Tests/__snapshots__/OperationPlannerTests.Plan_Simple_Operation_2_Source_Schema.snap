request:
  - document: >-
      {
        productById(id: 1) {
          id
          name
          estimatedDelivery(postCode: "12345")
        }
      }
nodes:
  - id: 1
    schema: "PRODUCTS"
    operation: >-
      {
        productById(id: 1) {
          id
          name
          id
        }
      }
  - id: 2
    schema: "SHIPPING"
    operation: >-
      query($__fusion_requirement_1: ID!) {
        productById(id: $__fusion_requirement_1) {
          estimatedDelivery(postCode: "12345")
        }
      }
    requirements:
      - name: "__fusion_requirement_1"
        dependsOn: "1"
        selectionSet: "productById"
        field: "id"
        type: "ID!"
