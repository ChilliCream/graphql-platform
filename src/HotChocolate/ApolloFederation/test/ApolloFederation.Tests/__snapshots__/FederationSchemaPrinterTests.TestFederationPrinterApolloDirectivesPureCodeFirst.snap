type Address {
  zipcode: String! @external
}

type QueryRootOfUser {
  entity(id: Int!): User!
}

"Scalar representing a JWT scope"
scalar Scope

type User @key(fields: "id") {
  id: Int!
  idCode: String! @external
  idCodeShort: String! @requires(fields: "idCode")
  address: Address! @provides(fields: "zipcode")
}
