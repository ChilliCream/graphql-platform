query foo(
  $v: Boolean!
  $q: Boolean!
) {
  hero(episode: EMPIRE) @include(if: $v) {
    name @include(if: $q)
  }
  ... on Query {
    hero(episode: EMPIRE) {
      id
    }
  }
  ... @include(if: $v) {
    hero(episode: EMPIRE) {
      height
    }
  }
}

