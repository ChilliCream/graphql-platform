query QueryName {
  oneOrTwo {
    ... TypeOneParts
    ... TypeTwoParts
  }
}

fragment TypeOneParts on TypeOne {
  field1
  field2
}

fragment TypeTwoParts on TypeTwo {
  field1
  field2
}

---------------------------------------------------------

query QueryName {
  ... on Query {
    oneOrTwo @__execute(id: 0, kind: DEFAULT, type: COMPOSITE) {
      ... on TypeTwo {
        field1 @__execute(id: 1, kind: DEFAULT, type: LEAF)
        field2 @__execute(id: 2, kind: DEFAULT, type: LEAF)
      }
      ... on TypeOne {
        field1 @__execute(id: 3, kind: DEFAULT, type: LEAF)
        field2 @__execute(id: 4, kind: DEFAULT, type: LEAF)
      }
    }
  }
}
