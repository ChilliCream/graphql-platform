schema {
  query: Query
  subscription: MySubscription
}

type MySubscription {
  onMessage(userId: String!): String!
  onSysMessage: String!
  onFixedMessage: String!
  onInferTopic: String!
  onExplicit: String!
  onExplicitNonGeneric: String!
  onExplicitNonGenericSync: String!
  onExplicitSync: String!
  onArguments(arg: String!): String!
}

type Query {
  a: String
}
