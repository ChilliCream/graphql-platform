}

input FooInput {
  contextData1: [KeyValuePairOfStringAndStringInput!]
  contextData2: [KeyValuePairOfStringAndStringInput!]
  contextData3: [KeyValuePairOfStringAndStringInput!]
}

input KeyValuePairOfStringAndStringInput {
