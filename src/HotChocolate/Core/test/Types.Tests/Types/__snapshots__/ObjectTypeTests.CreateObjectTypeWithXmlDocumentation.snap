schema {
  query: QueryWithDocumentation
}

"Query Documentation."
type QueryWithDocumentation {
  """
  Foo Documentation.
  
  
  **Returns:**
  
  Bar Returns Documentation.
  """
  foo("Bar Documentation." bar: String): String
  """
  This is a
  multiline summary
              
  with a newline in between.
  """
  baz: String
  "This is a single line summary."
  qux: String
}
