schema {
  query: Query
  mutation: Mutation
}

interface Bar {
  id: ID!
}

type Baz {
  name("some comment" baz: BazInput): String
}

type Foo implements Bar {
  id: ID!
}

type Mutation {
  mutate: String
}

type Query {
  foo: Foo
}

union FooOrBaz = Foo | Baz

input BazInput {
  name: String
}
