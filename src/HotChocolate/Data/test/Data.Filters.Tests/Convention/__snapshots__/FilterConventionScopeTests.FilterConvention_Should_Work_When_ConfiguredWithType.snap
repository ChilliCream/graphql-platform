schema {
  query: QueryType
}

type Foo {
  bar: String!
}

type QueryType {
  foos(where: ObjectFilterInput): [Foo]
  foosBar(where: Bar_ObjectFilterInput): [Foo]
}

input Bar_ObjectFilterInput {
  and: [Bar_ObjectFilterInput!]
  or: [Bar_ObjectFilterInput!]
}

input ObjectFilterInput {
  and: [ObjectFilterInput!]
  or: [ObjectFilterInput!]
}

"The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
scalar String
