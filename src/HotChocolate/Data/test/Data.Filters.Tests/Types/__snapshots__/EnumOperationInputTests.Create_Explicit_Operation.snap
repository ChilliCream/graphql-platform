schema {
  query: Query
}

type Query {
  foo(test: FooFilterTypeFilterInput): String
}

input EnumOperationInputOfFooBarFilterInput {
  eq: FooBar
  in: [FooBar!]
  neq: FooBar
  nin: [FooBar!]
}

input FooFilterTypeFilterInput {
  and: [FooFilterTypeFilterInput!]
  comparable: EnumOperationInputOfFooBarFilterInput
  or: [FooFilterTypeFilterInput!]
}

enum FooBar {
  FOO
  BAR
}

"The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
scalar String
