schema {
  query: QueryExplicit
}

type Bar {
  stringOperation: String!
}

type Foo {
  simpleMethod: Boolean!
  complexMethod: Bar!
}

type QueryExplicit {
  foos(where: Test): [Foo!]!
}

input BarFilterInput {
  and: [BarFilterInput!]
  or: [BarFilterInput!]
  stringOperation: StringOperationInputFilterInput
}

input BooleanOperationInputFilterInput {
  eq: Boolean
  neq: Boolean
}

input StringOperationInputFilterInput {
  and: [StringOperationInputFilterInput!]
  or: [StringOperationInputFilterInput!]
  eq: String
  neq: String
  contains: String
  ncontains: String
  in: [String]
  nin: [String]
  startsWith: String
  nstartsWith: String
  endsWith: String
  nendsWith: String
}

input Test {
  and: [Test!]
  or: [Test!]
  TestSimpleMethod: BooleanOperationInputFilterInput
  TestComplexMethod: BarFilterInput
}

"The `Boolean` scalar type represents `true` or `false`."
scalar Boolean

"The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
scalar String
