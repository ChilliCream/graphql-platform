SELECT [d].[Id], [d].[Bar]
FROM [Data] AS [d]
WHERE [d].[Bar] IS NULL OR (((@__p_0 <> N'') OR @__p_0 IS NULL) AND ([d].[Bar] IS NULL OR (@__p_0 IS NULL OR (((RIGHT([d].[Bar], LEN(@__p_0)) <> @__p_0) OR (RIGHT([d].[Bar], LEN(@__p_0)) IS NULL OR @__p_0 IS NULL)) AND (RIGHT([d].[Bar], LEN(@__p_0)) IS NOT NULL OR @__p_0 IS NOT NULL)))))
