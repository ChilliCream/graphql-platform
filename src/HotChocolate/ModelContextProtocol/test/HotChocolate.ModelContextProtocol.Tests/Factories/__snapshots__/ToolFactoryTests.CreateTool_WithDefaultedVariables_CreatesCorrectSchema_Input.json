    "string": {
      "type": "string",
      "description": "String description",
      "default": "default"
    },
    "timeSpan": {
      "type": "string",
      "pattern": "^-?P(?:\\d+W|(?=\\d|T(?:\\d|$))(?:\\d+Y)?(?:\\d+M)?(?:\\d+D)?(?:T(?:\\d+H)?(?:\\d+M)?(?:\\d+(?:\\.\\d+)?S)?)?)$",
