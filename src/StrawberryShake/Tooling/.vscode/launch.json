{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/dotnet-graphql/bin/Debug/net7.0/dotnet-graphql.dll",
      "args": [
        "generate",
        "/Users/michael/local/play/StrawberryTest",
        "-o /Users/michael/local/play/StrawberryTest/obj/Debug/net7.0/berry/",
        "-q /Users/michael/local/play/StrawberryTest/default",
        "-n StrawberryBuildTests",
        "-a md5",
        //"--relayFormat"
      ],
      "cwd": "${workspaceFolder}/src/dotnet-graphql",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
