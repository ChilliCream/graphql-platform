<Project>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)..\'))" />
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Nullable.props', '$(MSBuildThisFileDirectory)..\'))" />

  <PropertyGroup>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <Product>Strawberry Shake</Product>
    <PackageIcon>strawberryshake-signet.png</PackageIcon>
  </PropertyGroup>

  <PropertyGroup>
    <TestTargetFrameworks>net6.0; net5.0; netcoreapp3.1</TestTargetFrameworks>
    <LibraryTargetFrameworks>net6.0; net5.0; netcoreapp3.1; netstandard2.1; netstandard2.0</LibraryTargetFrameworks>
    <ToolingTargetFrameworks>net6.0; net5.0; netcoreapp3.1; netcoreapp2.1</ToolingTargetFrameworks>
  </PropertyGroup>

</Project>
