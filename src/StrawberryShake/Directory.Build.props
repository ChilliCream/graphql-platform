<Project>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)..\'))" />
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Nullable.props', '$(MSBuildThisFileDirectory)..\'))" />

  <PropertyGroup>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <Product>Strawberry Shake</Product>
    <PackageIcon>strawberryshake-signet.png</PackageIcon>
  </PropertyGroup>

  <PropertyGroup>
    <TestTargetFrameworks>net7.0; net6.0</TestTargetFrameworks>
    <LibraryTargetFrameworks>net7.0; net6.0; netstandard2.1; netstandard2.0</LibraryTargetFrameworks>
    <ToolingTargetFrameworks>net7.0; net6.0</ToolingTargetFrameworks>
  </PropertyGroup>

</Project>
