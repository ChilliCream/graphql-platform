public abstract partial MyClass
    : SomeOtherType
{
    private readonly Bar _foo;

    protected MyClass(Bar foo = default)
    {
        _foo = foo;
    }

    public Bar Foo
    {
        get => _foo
    }

    private ValueTask<Bar> GetFooAsync()
    {
        return new ValueTask<Bar>(_foo);
    }
}
