﻿public interface IHasName
{
    string? Name { get; }
}


public class Droid
    : IHasName
{
    public Droid(
        string? name)
    {
        name = Name;
    }
    public string? Name { get; }
}


public class Human
    : IHasName
{
    public Human(
        string? name)
    {
        name = Name;
    }
    public string? Name { get; }
}


