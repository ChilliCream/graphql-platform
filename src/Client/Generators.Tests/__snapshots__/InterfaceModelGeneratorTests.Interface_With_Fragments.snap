﻿public interface IHero
    : IHasName
    , IHasFriends
{
}


public interface IHasName
{
    string? Name { get; }
}


public interface IHasFriends
{
    UNKNOWN Friends { get; }
}


public class Hero
    : IHero
{
    public Hero(
        string? name,
        UNKNOWN friends)
    {
        name = Name;
        friends = Friends;
    }
    public string? Name { get; }

    public UNKNOWN Friends { get; }
}


